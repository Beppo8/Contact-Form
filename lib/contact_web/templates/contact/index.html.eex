<h1>Contact us</h1>

<%= form_for @changeset, Routes.contact_path(@conn, :create), fn f -> %>
    <%= label f, :email %>
    <%= text_input f, :email %>
    <%= error_tag f, :email %>
    <br>
    <%= label f, :message %>
    <%= textarea f, :message, rows: "3" %>
    <%= error_tag f, :message %>
    <br>
    <%= submit "Send Message" %>
<% end %>